<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="90"
        x:Class="AvaloniaApplication3.Views.RegexWindow"
        Title="RegexWindow" Width="400" Height="90">
	
	<WrapPanel>
		<TextBox Name="RegexInput" Height="30" Width="400"></TextBox>
		<Button Name="RegexOk" Height="30" Width="400" VerticalContentAlignment="Center" HorizontalContentAlignment="Center">Ok</Button>
		<Button Name="RegexCancel" Height="30" Width="400" VerticalContentAlignment="Center" HorizontalContentAlignment="Center">Cancel</Button>
	</WrapPanel>
	
</Window>
